<templates>
    <t t-name="ToothCust">
        <div>
            <div>
                <h3>
                    Tooth Custom
                </h3>
            </div>
            <div class="filter_view_tb"></div>
            <br></br>
            <div>
                <div class="table_view"></div>
            </div>
        </div>
    </t>
     <t t-name="ToothTable">
    <div class="tooth_selection">
        <t t-foreach="tooth_lines" t-as="data">
            <button class="tooth_button" t-att-data-id="data['id']" t-att-data-name="data['name']" t-att-data-image="data['image']">
                <img t-if="data['image']" t-att-src="'data:image/png;base64,' + data['image']" style="max-width:100%;"/>
            </button>
        </t>
    </div>
</t>

</templates>
